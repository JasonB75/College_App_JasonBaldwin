<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/familyTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="ProfileContent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/profilefirstNameView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="First"
        app:layout_constraintEnd_toEndOf="@+id/familyTextView"
        app:layout_constraintStart_toStartOf="@+id/familyTextView"
        app:layout_constraintTop_toBottomOf="@+id/familyTextView" />

    <TextView
        android:id="@+id/profilelastNameView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Last"
        app:layout_constraintEnd_toEndOf="@+id/familyTextView"
        app:layout_constraintHorizontal_bias="0.575"
        app:layout_constraintStart_toStartOf="@+id/familyTextView"
        app:layout_constraintTop_toBottomOf="@+id/profilefirstNameView" />


</android.support.constraint.ConstraintLayout>